{% extends 'myPage.html.twig' %}

{% block header %}

    <nav class="navbar navbar-dark bg-primary"><a class="navbar-brand " href="/tests">Тесты</a><a href="/new" class="navbar-brand">Создать тест</a><a class="navbar-brand">Регистрация</a><a class="navbar-brand">Главная</a></nav>
    <div class="boldLine"><span class="boldTitle"></span> <span class="boldTitleLast"> </span></div>
{% endblock %}


{% block lastTest %}

             {% for test in tests %}
                 <div class="list-group Qblock">
            <a class="list-group-item list-group-item-action flex-column align-items-start active" href="/tests/{{ test.id }}">
                <div class="d-flex w-100 justify-content-between"><h5 class="mb-1">{{ test.nameTest }}</h5>
              </div>
                <p class="mb-1">{{ test.title }}</p>
            </a>
                 </div>

            {% endfor %}



{% endblock %}